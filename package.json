{
    "name": "vscode-mlog",
    "displayName": "Mindustry Logic",
    "description": "Mindustry Logic (mlog) highlighting for VSCode!",
    "version": "0.0.2",
    "publisher": "Antyos",
    "icon": "images/logo.png",
    "license": "SEE LICENSE IN LICENSE",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "mindustry",
        "logic",
        "mlog"
    ],
    "homepage": "https://github.com/Antyos/vscode-mlog/blob/main/README.md",
    "bugs": {
        "url": "https://github.com/Antyos/vscode-mlog/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Antyos/vscode-mlog"
    },
    "contributes": {
        "languages": [
            {
                "id": "mlog",
                "aliases": [
                    "Mindustry Logic",
                    "mindustry logic",
                    "mlog"
                ],
                "extensions": [
                    ".mlog"
                ],
                "configuration": "./mlog-language-configuration.json"
            },
            {
                "id": "mpro",
                "aliases": [
                    "Mindustry Program",
                    "mindustry program",
                    "mpro"
                ],
                "extensions": [
                    ".mpro"
                ],
                "configuration": "./mpro-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mpro",
                "scopeName": "source.mpro",
                "path": "./syntaxes/mpro.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./",
        "update-grammar": "js-yaml syntaxes/mlog.yaml-tmLanguage > syntaxes/mlog.tmLanguage.json"
    },
    "devDependencies": {
        "@types/node": "^9.4.6",
        "@types/vscode": "^1.44.0",
        "js-yaml": "^3.13.1",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1",
        "vsce": "^1.75.0"
    },
    "dependencies": {}
}
